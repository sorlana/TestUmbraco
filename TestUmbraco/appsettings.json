          }
        },
        "ResolveUrlsFromText": false,
        "ForceSafeAliases": true
      },
      "Security": {
        "AllowPasswordReset": true,
        "KeepUserLoggedIn": false
      },
      "Hosting": {
        "Debug": true,
        "LocalTempStorageLocation": "Default"
      },
      "Runtime": {
        "Mode": "Development",
        "Minification": {
          "UseInMemoryCache": false,
          "CacheBuster": "Timestamp"
        },
        "TemplateFolders": ["~/Views"],
        "ModeValidation": false,
        "LogUnmatchedControllerActions": true
      },
      "Templates": {
        "DefaultRenderingEngine": "Razor",
        "EnableTemplateFolders": false,
        "UseViewEngineForTemplates": false,
        "AllowedDiscrepancies": {
          "ViewPaths": false,
          "TemplateNames": false
        },
        "CacheTemplateRedis": false,
        "TemplateCache": {
          "Enabled": false
        },
        "AllowTemplateOverride": true,
        "ResolveTemplateFromViewEngine": false,
        "DisablePhysicalFileLookup": true
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": true
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "http://localhost:5000/",
        "TrySkipIisCustomErrors": false,
        "UrlProviderMode": "Relative",
        "AllowFinderResolvers": true
      },
      "Unpublished": {
