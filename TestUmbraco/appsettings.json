{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Umbraco.Cms.Core": "Information",
        "Umbraco.Cms.Web.Common.Routing": "Information",
        "Umbraco.Cms.Web.Common.Controllers.RenderController": "Debug",
        "Umbraco.Cms.Core.Services.DocumentUrlService": "Debug",
        "Umbraco.Cms.Core.Packaging": "Debug",
        "YourProject.Controllers": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  
  "Umbraco": {
    "CMS": {
      "DistributedCache": {
        "Enabled": false,
        "DistributedCacheType": "NoCache"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsNamespace": "Umbraco.Cms.Web.Common.PublishedModels",
        "ModelsDirectory": "~/umbraco/models",
        "AcceptUnsafeModelsDirectory": true
      },
      "Global": {
        "InstallMissingDatabase": true,
        "InstallDefaultData": true,
        "UseHttps": false,
        "Id": "c447d94a-f4d2-41e6-b1ec-6c85afbc3762",
        "SanitizeTinyMce": true,
        "TimeOut": 90,
        "VersionCheckPeriod": 7
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true,
          "KeepAllVersionsNewerThanDays": 7,
          "KeepLatestVersionPerDayForDays": 30
        },
        "ResolveViewsFromCurrentProject": false,
        "AllowPreview": true,
        "Imaging": {
          "AutoFillImageFields": true,
          "ImageSharp": {
            "Cache": {
              "BrowserMaxAge": "7.00:00:00",
              "CacheFolder": "~/App_Data/TEMP/MediaCache",
              "CacheMaxAge": "365.00:00:00",
              "CacheHashLength": 8
