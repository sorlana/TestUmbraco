{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Umbraco.Cms.Core": "Information",
        "Umbraco.Cms.Web.Common.Routing": "Information",
        "Umbraco.Cms.Web.Common.Controllers.RenderController": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Global": {
        "Id": "ef511f7c-092a-45f5-b996-9bbd5cac5d62",
        "SanitizeTinyMce": true,
        "TimeOut": 90,
        "UseHttps": false,
        "VersionCheckPeriod": 7
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true,
          "KeepAllVersionsNewerThanDays": 7,
          "KeepLatestVersionPerDayForDays": 30
        },
        "ResolveViewsFromCurrentProject": true,
        "AllowPreview": true,
        "Imaging": {
          "AutoFillImageFields": true
        }
      },
      "Security": {
        "AllowPasswordReset": true,
        "KeepUserLoggedIn": false
      },
      "Hosting": {
        "Debug": false,
        "LocalTempStorageLocation": "Default"
      },
      "Runtime": {
        "Mode": "Development",
        "Minification": {
          "UseInMemoryCache": true
        },
        "TemplateFolders": ["~/Views"],
        "ModeValidation": false,
        "UseViewEngineForTemplates": true,
        "LogUnmatchedControllerActions": true
      },
      "Templates": {
        "DefaultRenderingEngine": "Razor",
        "EnableTemplateFolders": true,
        "UseViewEngine": true,
        "AllowedDiscrepancies": {
          "ViewPaths": true,
          "TemplateNames": true
        },
        "CacheTemplateRedis": false,
        "TemplateCache": {
          "Enabled": false
        },
        "AllowTemplateOverride": true,
        "ResolveTemplateFromViewEngine": true
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": true
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "http://localhost:5000/",
        "TrySkipIisCustomErrors": true,
        "UrlProviderMode": "Auto",
        "AllowFinderResolvers": true
      },
      "Unpublished": {
        "SupportUnpublished": true,
        "AllowPreview": true
      },
      "View": {
        "CompiledViewCacheEnabled": false,
        "ViewLocationCacheEnabled": false,
        "AllowRazorViewCompilation": true,
        "EnableRazorRuntimeCompilation": true
      },
      "Install": {
        "AutoComplete": true
      },
      "Examine": {
        "LuceneDirectoryFactory": "SyncedTempFileSystemDirectoryFactory"
      },
      "TypeFinder": {
        "AdditionalEntryAssemblies": [],
        "ExcludedTypeNames": []
      }
    }
  },
  
  "ConnectionStrings": {
