    {
      "contentTypeKey": "e91cd36f-4bdf-4c74-90bf-b2c85fd00dae",
      "udi": "umb://element/152a3c9b864343baa8e41ac48e1b903c",
      "imgMargin": "",
      "image": "[\r\n  {\r\n    \"key\": \"c3293dc9-33b3-4c00-af88-8cdba3324e96\",\r\n    \"mediaKey\": \"b262618b-3d07-4fb2-90ff-4dd8861db0c4\"\r\n  }\r\n]",
      "crop": "[\"Авто\"]",
