    {
      "contentTypeKey": "69de9db6-ec92-4989-a5f6-8437b8f3d5cd",
      "udi": "umb://element/48d95d0ba79b4b97aaded41595afc917",
      "richText": "<div class=\"count-platforms\" style=\"text-align: center;\">\n<div class=\"count-platforms\">The company's operating fleet includes <span class=\"vinous\"><strong>more than <span class=\"bigText\">2,500</span> units</strong></span> of logging platforms</div>\n</div>"
    },
