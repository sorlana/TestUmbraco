      "locationImage": "[\"Слева\"]",
      "image": "[\r\n  {\r\n    \"key\": \"77acf65b-2ea3-4664-856a-6ebf86f6d0dd\",\r\n    \"mediaKey\": \"53149261-7a07-4339-9d4f-0e2050a4abe6\"\r\n  }\r\n]",
      "sizeFoto": "4",
