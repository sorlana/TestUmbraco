{
  "mcpServers": {
    "qdrant-mcp": {
      "command": "node",
      "args": [
        "D:\\SITES\\My\\Qdrant_mcp\\qdrant-mcp-server\\build\\index.js"
      ],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "EMBEDDING_BASE_URL": "http://localhost:11434",
        "EMBEDDING_PROVIDER": "ollama",
        "EMBEDDING_MODEL": "nomic-embed-text"
      },
      "autoApprove": [
        "index_codebase",
        "get_collection_info",
        "list_collections",
        "delete_collection",
        "clear_index"
      ],
      "disabledTools": []
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "D:\\SITES\\Work\\umbraco-test-local"
      ],
      "env": {}
    }
  }
}